{
  "name": "uigradients",
  "version": "0.2.1",
  "description": "a uigradients.com generator react & styled-components.",
  "main": "dist/uigradients.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:dist",
    "build:lib": "babel src --out-dir lib",
    "build:dist": "rollup -c && rollup -c --environment PRODUCTION",
    "clean": "rm -rf dist/*",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "bugs": {
    "url": "https://garetmckinley.github.io/uigradients/issues"
  },
  "homepage": "https://garetmckinley.github.io/uigradients",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garetmckinley/uigradients.git"
  },
  "author": "Garet McKinley",
  "license": "MIT",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype-errors": "^3.3.6",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rollup": "^0.53.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-inject": "^2.0.0",
    "rollup-plugin-json": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "styled-components": "^2.4.0"
  },
  "dependencies": {
    "react": "^16.2.0",
    "styled-components": "^2.4.0"
  }
